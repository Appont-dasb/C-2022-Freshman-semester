#include<stdio.h>
int main()
{
	int arr[] = {1,2,3,4,5,6,7};
	printf("%p\n",arr);
	printf("%p\n",arr+1);

	printf("%p\n",&arr[0]);
	printf("%p\n",&arr[0]+1);

	printf("a = %p\n",&arr); //&arræ˜¯æ•°ç»„çš„åœ°å€
	printf("b = %p\n",&arr+1);
	//aå’Œbç›¸å·®28ï¼ˆarræ•°ç»„çš„å¤§å°ï¼‰
	return 0;

}


