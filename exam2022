#include<stdio.h>
int main()
{
  int arr[][4] = {1,2,3,4,5,6,7};
  //è¡Œçš„ä¸‹æ ‡å¯ä»¥ä¸å†™ï¼Œä½†åˆ—çš„ä¸‹æ ‡ä¸€å®šå¾—å†™
  return 0;
