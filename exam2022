//EOF
//EOF = end of file ->å€¼-1âœ¨
#include<stdio.h>

int main()
{
	int ch = 0 ;
	// when you input:ctrl + Z ,it will be over .
	while((ch=getchar())!=EOF)
	{
		putchar(ch) ;
	}
 return 0;
 
}


//do-while
//è¾“å…¥å¯†ç ç¨‹åº
#define _CRT_SECURE_NO_WARNINGS 1
#include<stdio.h>

int main()
{


	int ch = 0 ;
	int cet = 0 ;
	char password[16] = {0};
	printf("è¯·è¾“å…¥å¯†ç ï¼š\n");
	scanf("%s",password);

	while((ch=getchar())!='\n')
	{
		;
	}
        // ç¼“å†²åŒºè¿˜å‰©ä¸€ä¸ª'\n'
	// è¯»å–ä¸‹'\n'
	printf("è¯·ç¡®è®¤å¯†ç (Y\\N)ï¼š\n"); // ä¸¤ä¸ª\\
	cet = getchar() ; // Y/N
	do
	{
		
	if(cet == 'Y')
	{
		printf("ç¡®è®¤æˆåŠŸ\n");
		break;
	}
	else if(cet =='N')
	{
		printf("è¯·å†æ¬¡è¾“å…¥å¯†ç ï¼š\n");
		
	    scanf("%s",password);
		while((ch=getchar())!='\n')
	{
		;
	}
		printf("è¯·å†æ¬¡ç¡®è®¤å¯†ç (Y\\N)ï¼š\n");
		cet = getchar(); //è¿™é‡Œå¦‚è‹¥è¾“å…¥Nï¼Œåˆ™å°†å¾ªç¯ä¸‹å»ï¼Œç›´åˆ°å¯†ç ç¡®è®¤æˆåŠŸã€‚

	}
	}while(cet != 'Y'); //å…ˆå¾ªç¯å†åˆ¤æ–­
	return 0;
}


//è½¬è½½âœ¨
//å¦ä¸€ç§å¯†ç è¾“å…¥ç³»ç»Ÿ
//strcmpåº“å‡½æ•°çš„åº”ç”¨
#include <string.h>
#include <stdio.h>
int main()
{
	char arr[20] = { 0 };
	//å‡è®¾æ­£ç¡®çš„å¯†ç æ˜¯å­—ç¬¦ä¸²â€œ2022â€ï¼Œå¦‚ä½•æ¯”è¾ƒï¼Ÿ
	int m = 0;
	for (m = 0; m < 3; m++)
	{
		printf("è¯·è¾“å…¥å¯†ç ï¼š");
		scanf("%s", arr);
		//if (arr == "2022")//è¿™æ ·çš„å†™æ³•æ˜¯é”™è¯¯çš„ï¼Œä¸¤ä¸ªå­—ç¬¦ä¸²æ¯”è¾ƒä¸èƒ½ä½¿ç”¨==æ¥æ¯”è¾ƒ
		//åº”è¯¥ä½¿ç”¨å­—ç¬¦ä¸²å‡½æ•°strcmp
		if(strcmp(arr,"2022")==0)
		{

			printf("å¯†ç æ­£ç¡®\n");
			printf("è¿›å…¥ç³»ç»Ÿ");
			break;
		}
			else
			{
				printf("å¯†ç é”™è¯¯\n");
			}
		
	}
	if (m == 3)
	{
		printf("é€€å‡ºç³»ç»Ÿ\n");//æœ€å¤šè¾“å…¥ä¸‰æ¬¡å¯†ç 
	}
	return 0;
}


//ä»£ç ä¸€å®šè¦è‡ªå·±æ•²ä¸€é
#include<stdio.h>
#include<string.h>
int main()
{
	int i = 0 ;

	char password[20] ={ 0 };

	for(i=0; i<3; i++)
	{
			printf("è¯·è¾“å…¥å¯†ç ï¼š\n");
		scanf("%s",password);
		if(strcmp(password,"2022") == 0) //è¿™é‡Œçš„strcmpæ˜¯ä¸€ä¸ªåº“å‡½æ•°ï¼Œå¯ä»¥ç”¨æ¥æ¯”è¾ƒä¸¤ä¸ªå­—ç¬¦ä¸²æ˜¯å¦ç›¸ç­‰ã€‚
		//è‹¥ç›¸ç­‰ï¼Œåˆ™strcmp(password,"2022")çš„å€¼ä¸º0.
		//å¦‚æœç¬¬ä¸€ä¸ªpasswordå¤§äºç¬¬äºŒçš„"2022"ï¼Œè¾“å‡ºä¸€ä¸ªå¤§äº0çš„æ•°
		//å¦‚æœç¬¬ä¸€ä¸ªpasswordå°äºç¬¬äºŒçš„"2022"ï¼Œè¾“å‡ºä¸€ä¸ªå°äº0çš„æ•°
		{
			printf("å¯†ç æ­£ç¡®\n");
			break ;
		}
		else
		{
			printf("å¯†ç é”™è¯¯\n");
		}
	}
	if(i == 3)
	{
		printf("é€€å‡ºç³»ç»Ÿ\n");
	}
	return 0 ;

}
